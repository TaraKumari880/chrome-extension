{
    "name": "Ask any query",
    "description": "A chrome extension for efficient internet browsing",
    "version": "1.0",
    "manifest_version": 3,
    "author": "tara@kumari.io",
    "icons": {
        "16": "images/icon.png",
        "48": "images/icon.png",
        "128": "images/icon.png"
    },
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "images/icon.png",
            "48": "images/icon.png",
            "128": "images/icon.png"
        },
        "default_title": "My Amazing Chrome Extension"
    },
    
    "content_scripts": [
        {
            "js": [ "scripts/popup.js", "scripts/content.js"
            ],
            "css": [
                "scripts/content.css"
            ],
            "matches": [
                "*://*/*"
                
            ]
        }
    ]
}